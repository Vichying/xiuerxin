body{background: url("../images/login/img_beij.png");background-size: 100% 100%;height: 100%;width: 100%}

header{width: 100%;height: auto;background: #014298;position: relative;z-index: 2;}
header .logoBox{width: 1200px;height: 70px;margin: 0 auto;}
header .logoBox img{vertical-align: middle;margin: 13.5px 60px 13.5px 0;}
header .logoBox p{font-size: 16px;color: #fff;line-height: 70px;}

/* 注册登录公共样式 */
main{margin-top: 100px;}
.formBox button{background-color: #1164DF;font-size: 16px;color: #fff;}
.formBox button:hover{background-color: #FF9000;}
.formBox{position: relative;left: 50%;margin-left: -280px;width: 560px;height: 600px;background: #FFFFFF;box-shadow: 0px 15px 50px 0px rgba(103, 182, 206, 0.27);border-radius: 6px;padding: 40px 60px;}
.formBox .title{font-size: 24px;text-align: center;padding-bottom: 20px;border-bottom: 1px solid #ccc;margin-bottom: 30px;}
.formBox .inputDiv{position: relative;margin-bottom: 25px;}
.formBox input{width: 100%;height: 46px;background: #FFFFFF;border: 1px solid #CCCCCC;padding: 0 15px;position: relative;}
.formBox .inputDiv p{position: absolute;}
.formBox .verificationCode input{width: 270px;}
.formBox .iconBox{display: inline-block;position: absolute;top: 6px;right: 13px;}
.formBox .verificationCode button{width: 150px;height: 46px;}
.formBox .submitBtn{width: 440px;height: 46px;border-radius: 2px;}
.tipCode{margin-top: -25px;}
.timeBox{width: 150px;height: 46px;background-color: #1164DF;font-size: 16px;color: #fff;text-align: center;line-height: 46px;}
/* 用户协议 */
.userAgreement{margin-top: 25px;}
.userAgreement img{margin: 3px 8px 0 0;}
.userAgreement img:hover{cursor: pointer;}
.userAgreement .blueText:hover,.userAgreement .right:hover
{cursor: pointer;color: #FF9000;}
.userAgreement .right{color: #1164DF;}

/* 登录 */
.loginToReg{margin-bottom: 35px;}
.thirdParty{width: 350px;margin: 0 auto;height: 34px;}
.thirdParty .text{font-size: 24px;color: #666;margin: 0 15px;margin-top: -17px;}
.thirdParty .line{width: 100px;height: 1px;background: #999999;}
.thirdPartyBox .col-lg-4{text-align: center;}
.thirdPartyBox .col-lg-4 img{margin-bottom: 6px;}
#cvs{
  margin-left: 18px;
}
