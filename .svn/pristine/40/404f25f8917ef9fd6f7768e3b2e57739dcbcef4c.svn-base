main{width: 1050px;margin: 0 auto;padding-top: 60px;}
.tabBox ul li,.buttonBox{text-align: center;}
.tabBox ul li:hover,.buttonBox:hover{cursor: pointer;}
#creTipLeftText{border: 2px solid rgba(104, 110, 136, 0.4);height: 46px;line-height: 46px;width: 151px;padding-left: 0;padding-right: 0;}
#creTipLeftText input{width: 100%;}
#creInput{border: 2px solid rgba(104, 110, 136, 0.4);border-left: 0;height: 46px;line-height: 46px;border-right: 0;}
#identify{width: 779px;}
#creInput2{width: 930px;border-top: 2px solid rgba(104, 110, 136, 0.4);border-bottom: 2px solid rgba(104, 110, 136, 0.4);border-left: 2px solid rgba(104, 110, 136, 0.4);height: 46px;line-height: 46px;}
#creditInputBox{margin-bottom: 60px;}
#creditInputBox .buttonBox{background-color: #1164DF;}
#creditInputBox .buttonBox:hover{background-color: #FFA825;}

.noDataBox{width: 640px;margin: 0 auto;padding-bottom: 300px;}
.noDataBox .left{font-size: 20px;line-height: 52px;}
.noDataBox button{width: 160px;height: 52px;line-height: 52px;color: #fff;font-size: 16px;background: #1164DF;}
.noDataBox button:hover{background: #FF9000;}

.listBox .topListBox{margin-bottom: 60px;}
.topListBox .topListTitle{height: 68px;line-height: 68px;background-color: #1164DF;padding-left: 20px;color: #fff;font-size: 18px;}
.listBox .itemBox{padding: 33px 30px;border: 1px solid #DDDDDD;}
.checkInputBox{width: 18px;height: 18px;}
.checkbox-inline input[type=checkbox]{width: 18px;height: 18px;}
.textTitle h4{margin-bottom: 12px;width:365px;white-space: nowrap;overflow: hidden;text-overflow: ellipsis;}
.textTitle:hover h4{color: #1164DF;cursor: pointer;}
.textTitle{margin-right: 90px;}
.textTitle p{width:365px;white-space: nowrap;overflow: hidden;text-overflow: ellipsis;font-size: 16px;color: #666;}
.topListBox .itemBox .right{width: 120px;height: 36px;line-height: 36px;color: #fff;font-size: 16px;background: #1164DF;margin-top: 12px;}

.mainsBox .tabTitlesBox{width: 220px;margin-right: 20px;}
#accordion .panel-default>.panel-heading{height: 68px!important;padding: 0!important;text-align: center!important;}
#accordion .panel-default>.panel-heading h4 a:hover{color: #333!important;background-color: #1164DF!important;color: #fff!important;}
#accordion .panel-default>.panel-heading h4{line-height: 68px!important;color: #333;}
#accordion .panel-default>.panel-heading h4 a{display: block;}
.panel-group .panel{border-radius: 0!important;}
#accordion .panelActive{background-color: #1164DF;}
#accordion .panelActive h4 a{color: #fff;}
#accordion .panel-body li{height: 52px;line-height: 52px;font-size: 16px;padding-left: 35px;}
#accordion .panel-body li:hover{color: #1164DF;cursor: pointer;}
#accordion .panel-body .activLi{color: #1164DF;}

.mainsBox .bottomListBox{width: 810px;}
.bottomListBox .topListTitle{height: 68px;line-height: 68px;background-color: #1164DF;padding-left: 20px;color: #fff;font-size: 18px;border: 1px solid #ddd;text-align: center;}
.bottomListBox .itemBox{margin-top: -1px;}
.bottomListBox .itemBox .right{width: 120px;height: 36px;line-height: 36px;color: #1164DF;font-size: 16px;background: #fff;margin-top: 12px;border: 1px solid #1164DF;}
.itemBox .right:hover{background-color: #FF9000;border-color:  #FF9000;color: #fff;}
.itemBox .seriousBreach{border: 0!important;color: #FF9000!important;}
.itemBox .seriousBreach:hover{background-color: #fff!important;border: 0!important;color: #FF9000!important;}
/* 全选 */
.checkAllBox{padding: 30px;border: 1px solid #ddd;margin-top: -1px;margin-bottom: 80px;}
.checkAllBox .checkAllText,.checkAllBox .checkedNum{line-height: 44px;}
.checkAllBox .checkAllText{font-size: 18px;margin-right: 22px;}
.checkAllBox .checkedNum{font-size: 16px;color: #666;}
.checkAllBox .btnText{width: 120px;height: 36px;line-height: 36px;color: #fff;font-size: 16px;margin-top: 12px;background-color: #1164DF;}
.checkAllBox .btnText:hover{background-color: #FF9000;border-color:  #FF9000;color: #fff;}
.modal-dialog{margin-top: 240px;}
.modal-dialog .modal-body p{margin-bottom: 20px;color: #666;}

/* 案件详情 */
.detailTableBox .title{font-size: 18px;margin-bottom: 20px;}
.detailTableBox table{width: 100%;margin-bottom: 40px;}
.detailTableBox tr td{border: 1px solid #ddd;padding: 13px 15px;font-size: 16px;}
.detailTableBox tr td:first-child{width: 200px;}
.detailTableBox tr td .right{color: #1164DF;}
.detailTableBox tr td .right:hover{cursor: pointer;}
.btnsBox{text-align: center;margin-bottom: 60px;}
.btnsBox button{width: 200px;height: 52px;line-height: 52px;font-size: 16px;border: 1px solid #1164DF;}
.btnsBox button:hover{background-color: #FF9000;color: #fff;border: 1px solid #FF9000;}
.btnsBox .successBtn{color: #1164DF;border: 1px solid #1164DF;margin-right: 20px;background-color: #fff;}
.btnsBox .lineRestortionBtn{color: #fff;background: #1164DF;}
.modal-body{padding: 25px;}
.modal-body .title{font-size: 16px;margin-bottom: 20px;}
.modal-body table{width: 100%;margin-bottom: 30px;}
.modal-body table tr td{padding: 15px 20px;border: 1px solid #ddd;}
.modal-body table tr td:first-child{width: 220px;}
.modal-body .accumulate{text-align: center;color: #1164DF;font-size: 16px;}

/* 充值套餐 */
.topPositionBox{padding-top: 65px;margin-bottom: 30px;}
.topPositionBox p{font-size: 16px;}
.topPositionBox p span{color: #1164DF;}
.packageListBox .title{font-size: 28px;font-weight: bold;text-align: center;margin-bottom: 40px;}
.packageListBox .line{width: 100%;border-bottom: 1px solid #ddd;margin-bottom: 30px;}
.packageListBox .subTitle{font-size: 20px;margin-bottom: 20px;}
.packageListBox .packageBox{display: flex;display: -webkit-flex;justify-content: start;flex-wrap: wrap;margin-bottom: 60px;}
.packageListBox .packageBox .itemBox{width: 246px;margin-right: 22px;border: 1px solid #DDD;border-radius: 4px;padding: 24px 22px;margin-bottom: 22px;position: relative;}
.packageListBox .packageBox .itemBox .checkedIcon{position: absolute;    right: 0;bottom: 0;display: none;}
.packageListBox .packageBox .itemBox:hover{border-color: #FF9000;cursor: pointer;}
.packageListBox .packageBox .itemBox p{font-size: 16px;margin-bottom: 10px;}
.packageListBox .packageBox .itemBox div{color: #FF9000;font-size: 18px;}
.packageListBox .packageBox .itemBox div span{font-size: 26px;}
.payBtnBox{margin-bottom: 30px;}
.payBtnBox .text{font-size: 16px;margin-right: 28px;}
.payBtnBox .text span{font-size: 24px;color: #FF9000;}
.payBtnBox .text .money{font-size: 34px;color: #FF9000;}
.payBtnBox button{width: 176px;height: 50px;line-height: 50px;color: #fff;font-size: 16px;background: #1164DF;}
.payBtnBox button:hover{background-color: #FF9000;}
/* 一般失信/严重失信 在线修复弹窗 */
#dishonesty .modal-body p span a{color: #1164DF;}
.modal-footer{text-align: center;}
.modal-footer .btn-default{border: 1px solid #ddd!important;}
.modal-footer .btn-primary {color: #fff!important;background-color: #1164DF!important;border-color: #1164DF!important;}
.modal-dialog .modal-body .fixpopup p {padding: 0;margin:0; }
.modal-dialog .modal-body .serious-tip .title{color:#FF9000;}
.modal-dialog .modal-body .serious-tip a {color:#1164DF;}
