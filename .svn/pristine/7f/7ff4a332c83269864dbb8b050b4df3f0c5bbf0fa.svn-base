body{background-color: #F7F8FA;}
main{width: 1050px;margin: 0 auto;padding-top: 60px;}
.topPositionBox{padding-top: 65px;margin-bottom: 30px;}
.topPositionBox p{font-size: 16px;}
.topPositionBox p span{color: #1164DF;}
.applicationBox{background-color: #fff;padding: 22px 30px;margin-bottom: 50px;}
.applicationBox .title{font-size: 18px;margin-bottom: 20px;}
.descBox{margin-bottom: 30px;}
.descBox span a{color: #1164DF;}
.payInfoBox .theadBox{border: 1px solid #ddd;}
.payInfoBox .theadBox div{padding: 16px 24px;background: #F7F8FA;width: 50%;font-size: 18px;}
.payInfoBox .tbodyBox{border-right: 1px solid #ddd;border-left: 1px solid #ddd;}
.payInfoBox .tbodyBox div{padding: 16px 24px;width: 50%;font-size: 18px;}
.payInfoBox .tfooterBox{border: 1px solid #ddd;padding: 20px 25px;margin-bottom: 30px;}
.payInfoBox .tfooterBox .payTypeTitle{font-size: 18px;margin-bottom: 40px;}
.payInfoBox .tfooterBox .qrcodeBox{text-align: center;}
.payInfoBox .tfooterBox .qrcodeBox .qrcodeBorder{border: 1px dashed #000;width: 180px;height: 180px;padding: 4px;margin: 0 auto;margin-bottom: 20px;}
.payInfoBox .tfooterBox .qrcodeBox p{font-size: 16px;}
.payInfoBox .changePatType{color: #1164DF;font-size: 16px;}
.payInfoBox .changePatType:hover{cursor: pointer;}
/* 修复申请成功 */
.line{width: 100%;border-bottom: 1px solid #ddd;}
.topPositionBox .title{font-size: 18px;margin-bottom: 20px;font-weight: bold;}
.mainInfoBox{text-align: center;padding: 90px 0 60px;}
.topIconBox{width: 120px;height: 120px;margin: 0 auto;margin-bottom: 30px;}
.successDesc{font-size: 18px;font-weight: bold;margin-bottom: 60px;}
/* -------------优惠券-------------------- */
#cunponBox {margin: 0 auto;width: 480px;display: flex;background:#ffc84f;}
/*前半部分样式*/
#cunponBox .content {position: relative;flex: 1;padding: 7px 28px;text-align: center;}
/*后半部分样式*/
#cunponBox  .tip {position: relative;flex: 0 0 280px;text-align: center;}
/*中间竖直虚线样式*/
#cunponBox .split-line {position: relative;flex: 0 0 0;margin: 0 10px 0 6px;border-left: 2px dashed #fff;   }
/*给虚线加两个伪类，基本样式如下*/
#cunponBox .split-line:before, .split-line:after {content: '';position: absolute;width: 32px;height: 16px;background: #fff;left: -18px;z-index: 1;}
/*给前半部分加两个伪类，基本样式如下*/
/* #cunponBox .content:before, .content:after{content: '';position: absolute;width: 32px;height: 16px;background: #fff;left: -16px;z-index: 1;} */

/*给前半部分加两个伪类，基本样式如下*/
/* #cunponBox .tip:before,#cunponBox  .tip:after{content: '';position: absolute;width: 32px;height: 16px;background:#fff;right: -16px;z-index: 1;} */
/*几个伪类化成的圆弧的样式以及位置（置于顶部）我把它放在一起了*/
#cunponBox .content:before,#cunponBox  .tip:before,#cunponBox  .split-line:before{border-radius: 0 0 16px 16px;top: 0;}

/*几个伪类化成的圆弧的样式以及位置（置于底部）我把它放在一起了*/
#cunponBox .content:after,#cunponBox  .tip:after,#cunponBox  .split-line:after{border-radius: 16px 16px 0 0;bottom: 0;}
#cunponBox .money {font-size: 50px;color: #fff;font-weight: bold;}
#cunponBox .money text {font-size: 24px;padding-right: 10px; }
#cunponBox .title {color: #fff;font-size: 24px;}
#cunponBox .conditions {color: #fff;font-size: 24px;padding:25px 30px 20px 30px;}
#cunponBox .timeText{font-size: 20px;font-weight: bold;color: #fff;padding: 12px 0;}
#cunponBox .co {padding-top:10px; }
/* -------------优惠券-------------------- */
.cunponDesc{font-size: 16px;margin-top: 60px;margin-bottom: 40px;}
.riskMonitoring{padding: 15px 20px;background: #1164DF;color: #fff;margin-bottom: 80px;}
.riskMonitoring:hover{background-color: #FF9000;}
.erweimaBox{width: 202px;height: 202px;border: 1px dashed #000;padding: 4px;margin: 0 auto;margin-bottom: 28px;}
.qrcodeDesc{font-size: 16px;}
.checkOrder{background-color: #fff;border: 1px solid #1164DF;color: #1164DF;margin-right: 20px;}
.checkOrder:hover{background-color: #FF9000;border: 1px solid #FF9000;color: #fff;}