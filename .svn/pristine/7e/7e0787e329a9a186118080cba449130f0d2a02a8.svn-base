<script>
  function applicationFun() {
    console.log("申请异议")
  }
  $(function () {
    console.log(window.location.href.split("?flag=")[1])
    // 1可修复，0不可修复
    let flag = window.location.href.split("?flag=")[1]
    if (flag == 0) {
      $("#noResBtn").show()
      $("#resBtn").hide()
      $("#application").show()
    } else {
      $("#noResBtn").hide()
      $("#resBtn").show()
      $("#application").hide()
    }
  })
  // 查询框
  $(".tabBox ul li").click(function () {
    console.log($(this).index() == 0 ? '个人' : '企业')
    $(this).addClass('tabActive');
    $(this).siblings().removeClass('tabActive');
    if ($(this).index() == 0) {
      $("#creTipLeftText").show()
      $("#creInput").show()
      $("#creInput2").hide()
    } else {
      $("#creTipLeftText").hide()
      $("#creInput").hide()
      $("#creInput2").show()
    }
  })
  // 在线修复   0一般失信，1严重失信
  function toApplicationFn(val) {
    if (val == 0) {
      $("#commonly").show()
      $("#serious").hide()
    } else {
      $("#commonly").hide()
      $("#serious").show()
    }
  }
  function toRestorationFn() {
    window.location.href = "./repairApplication.html"
  }
</script>