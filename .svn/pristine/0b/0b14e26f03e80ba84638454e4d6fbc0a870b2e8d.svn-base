.contentBox{padding: 30px 62px;background-image: url("../../static/images/reporPDF/img_beijing_pdf.png");background-size: 100%;background-position: center;}
header {border-bottom: 1px dashed #999999;padding: 30px 0 15px;}
table {width: 100%;margin: 0 auto;}
table tr td{border: 0;}
.headFirstHeadTd{width: 80%;}
.headSecendHeadTd{width: 20%;text-align: right;}
.headSecendHeadTd div{margin-top: 8px;}

.topTitleBox{margin-top: 165px;}
.topTitleBox thead tr th div{font-size: 56px;text-align: center;}
.topTitleBox thead .titleFirstTr div{margin-bottom: 50px;}

.topInfoBox{width: 430px;margin: 290px auto 190px;}
.topInfoBox tbody tr td{font-size: 26px;padding: 5px 0;}
.topInfoBox tbody tr td:first-child{font-weight: bold;}
.topInfoBox tbody tr td:last-child{padding-left: 30px;}

.borderTable tr td{padding: 20px 25px;border: 1px solid #000;font-size: 26px;}
.borderTable tr td:nth-child(odd){font-weight: bold;}

.topTimeTable{width: 640px;margin: 0 auto 180px;}
/* 信用修复介绍 */
.descTitle{text-align: center;font-size: 50px;font-weight: bold;margin-bottom: 50px;}
.secendDescTitleBox{margin-bottom: 850px;}
.secendDescTitleBox .descTitle{margin-bottom: 66px;}
.secendDescTitleBox .descBox{font-size: 22px;line-height: 70px;text-indent: 2em;}
/* 公司声明 */
.companyStatement{margin-bottom: 265px;}
.descBoxItem{font-size: 22px;line-height: 70px;}
/* 目录 */
.catalogBox{margin-bottom: 400px;}
.catalogBox .firstTitletext{font-size: 24px;font-weight: bold;margin-bottom: 40px;width: 100%;height: 31px;position: relative;}
.secendTitleTextBox{margin-bottom: 58px;width: 100%;text-align:justify;text-justify:inter-ideograph;}
.secendTitleTextBox .secendTitleText{margin-bottom: 25px;font-size: 20px;position: relative;height: 26px;}
.catalogBox .firstTitletext span,.secendTitleTextBox .secendTitleText span{position: absolute;left: 0;z-index: 2;background-color: #fff;}
.catalogBox .firstTitletext s,.secendTitleTextBox .secendTitleText s{display: inline-block;height: 100%;width: 106%;position: absolute;left: 0;bottom: 3px;border-bottom: 3px dotted black;}
/* 基础信息 */
.baseInfoTable,.baseInfoTable2{margin-bottom: 60px;}
.baseInfoTable tr td:first-child{width: 290px;}
.subTitleText{font-size: 24px;font-weight: bold;margin-bottom: 24px;}
.baseInfoTable2 tr td:first-child{width: 60px;}
.baseInfoTable2 tr td{font-weight: normal!important;}
.baseInfoTable2 tr:first-child td{font-weight: bold!important;}

.infoDataBox{padding-left: 60px;}
.infoNumText{font-size: 24px;margin-bottom: 30px;}

.opinionBox{border: 1px solid #000;margin-bottom: 50px;}
.opinionBox .opinion{padding: 21px 17px;font-size: 20px;margin-bottom: 180px;}
.opinionBox .autograph{padding: 21px 17px;border-top: 1px solid #000;}
.opinionBox .autograph .left{font-weight: bold;font-size: 20px;}
.opinionBox .autograph .right{font-size: 20px;}
.userDescBox{font-size: 20px;line-height: 32px;}

/* 个人-基本信息 */
#personalBaseInfo{margin-bottom: 100px;}
.personalImgBox{width: 950px;margin: 0 auto;}
.personalImgBox img{width: 451px;}

table img{max-width: 100%;}