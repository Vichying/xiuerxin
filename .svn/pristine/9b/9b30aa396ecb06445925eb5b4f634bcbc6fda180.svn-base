/* 右侧漂浮导航开始 */
.rightFload{ position: fixed; width: 65px; right: 0; top: 50%; z-index: 200;}
.rightFload .pf_div{ float: left; width: 65px; height: 180px; position: relative;}
.rightFload .pf_div .divBox{ position: absolute; width: 65px; height: 188px; left: 0; top: 0; overflow: hidden;
  -webkit-transition: all 0.3s linear;
  -moz-transition: all 0.3s linear;
  -o-transition: all 0.3s linear;
  transition: all 0.3s linear;
}
.rightFload .pf_div .divBox.active{ left: 70px;}
.rightFload .pf_div .divBox li{ float: left; width: 65px; height: 55px;background-color: #1164DF;margin: 1px;border-radius:4px;
  -webkit-border-radius:4px;
  -moz-border-radius:4px;
  -o-border-radius:4px;
  text-align: center;
  color: #fff;
}
.rightFload .pf_div .divBox li:hover{color: #fff;cursor: pointer;}
.rightFload .pf_div .divBox li img{margin-top: 7px;}
.rightFload .pf_div .divBox li p{font-size: 10px;line-height: inherit;}
.rightFload .pf_div .tele{ 
  display: none; 
  position: absolute; right: 62px; top:1px; width: 160px; height: 55px; background: #FF9000; color: #fff; font-size: 14px; line-height: 55px; padding: 0 0 0 20px;
  border-radius:6px 0 0 6px;
  -webkit-border-radius:6px 0 0 6px;
  -moz-border-radius:6px 0 0 6px;
  -o-border-radius:6px 0 0 6px;
}
.rightFload .pf_div .wxEwm{ 
  display: none; 
  position: absolute; right: 62px; top:58px; width: 110px; padding: 10px 25px 10px 15px; background: #FF9000; color: #fff; font-size: 18px; font-weight: bold; line-height: 92px;
  border-radius:6px 0 0 6px;
  -webkit-border-radius:6px 0 0 6px;
  -moz-border-radius:6px 0 0 6px;
  -o-border-radius:6px 0 0 6px;
}
.rightFload .pf_div .qqEwm{ 
  display: none; 
  top:120px; background: #3491dd; }
.rightFload .pf_shut{ float: left; width: 26px; height: 26px; margin: 25px 0 0 17px; cursor: pointer;}
.rightFload .pf_shut img{
  -webkit-transition: all 0.3s linear;
  -moz-transition: all 0.3s linear;
  -o-transition: all 0.3s linear;
  transition: all 0.3s linear;
}
.rightFload .pf_shut.active img{
  transform:rotate(45deg);
  -webkit-transform:rotate(45deg);
  -moz-transform:rotate(45deg);
  -o-transform:rotate(45deg);
}
/* 右侧漂浮导航结束 */
