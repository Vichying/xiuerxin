.bannerBox {height: 666px;}
.bannerBox img{height: 560px;}
.companyDesc{width: 1050px;margin: 60px auto 68px;}
.companyDesc h3{font-size: 28px;font-weight: bold;text-align: center;margin-bottom: 36px;}
.companyDesc .descText{font-size: 18px;color: #666;line-height: 32px;}
/* 企业资质  发展历程 */
.qualification{height: 1066px;background-color: #fff;background-image: url("../images/img_beij_about.png");background-size: cover;background-position: center;padding: 60px 0 80px;}
.qualificationBox{width: 1050px;margin: 0 auto;}
.qualification h3{font-size: 28px;font-weight: bold;text-align: center;}
.qualificationTitle{color: #fff;margin-bottom: 36px;}
.qualificationImgBox{height: 386px;margin-bottom: 90px;}
.developmentDesc{margin-top: 48px;}
.developmentHistoryBox .subTitle{font-size: 18px;margin-bottom: 36px;}
.historyBox .dotBox{width: 24px;height: 24px;border-radius: 50%;border: 4px solid #1164DF;opacity: 0.7;margin-right: 15px;}
.historyBox .descText{font-size: 18px;color: #666;max-width: 1010px;}
.developmentDesc .columLine{border-left: 4px solid #1164DF;height: 30px;opacity: 0.7;position: relative;left: 10px;}

.enterpriseTrends{padding: 60px 0 0;background-color: #F7F8FA;margin-top: 60px;}
.enterpriseTrendsBox{width: 1050px;margin: 0 auto;}
.enterpriseTrendsBox h3{font-size: 28px;font-weight: bold;text-align: center;margin-bottom: 60px;}
/* 企业动态 */
#dynamics{width: 100%;height: 424px;}
#dynamics .dynamicsItemsBox{display: flex;display: -webkit-flex;justify-content: space-between;width: 1050px;margin: 0 auto;}
#dynamics .itemBox{width: 247px;}
#dynamics .itemBox .imgBox{width:100%;height: 170px;background-color: #ddd;overflow: hidden;}
#dynamics .itemBox .textBox{padding: 15px;background-color: #fff;}
#dynamics .itemBox .textBox .title{margin-bottom: 15px;font-size: 16px;width: 100%;white-space: nowrap;overflow: hidden;text-overflow: ellipsis;}
#dynamics .itemBox:hover .textBox .title{color: #FF9A23;}
#dynamics .itemBox:hover{cursor: pointer;}
.moreTrendsBtn{width: 200px;text-align: center;margin: 50px auto 0;height: 52px;line-height: 52px;font-size: 16px;color: #1164DF;border: 1px solid #1164DF;}
.moreTrendsBtn:hover{cursor: pointer;background-color:#FF9A23;color: #fff;border-color: #FF9A23;}
/* 合作伙伴 */
.cooperativePartner{padding: 60px 0;height: 587px;}
.cooperativePartnerBox{width: 1050px;margin: 0 auto;}
.cooperativePartnerBox h3{font-size: 28px;font-weight: bold;text-align: center;margin-bottom: 46px;}
.cooperativePartnerBox .childItemBox{display: flex;display: -webkit-flex;justify-content: space-between;flex-wrap: wrap;}
.childItemBox{margin-bottom: 16px;}
/* 联系我们 */
.contactUs{padding: 60px 0;background-color: #F7F8FA;}
.contactUs h3{font-size: 28px;font-weight: bold;text-align: center;margin-bottom: 40px;}
.contactUsBox{width: 1050px;margin: 0 auto;}
.contactUsBox .subBannerBox{height: 200px;margin-bottom: 20px;}
.bottomDescBox .itemBox{width: 515px;background-color: #fff;padding: 32px 40px;height: 201px;}
.bottomDescBox .itemBox h2{font-size: 20px;font-weight: bold;margin-bottom: 22px;}
.bottomDescBox .itemBox p{font-size: 16px;color: #666;margin-bottom: 3px;}
.bottomDescBox .itemBox h1{font-size: 22px;font-weight: bold;margin-bottom: 20px;}