<script>
  // 判断有无查询次数   1--有次数，2--无次数（去充值）
  let dataFlag = 1
  if (dataFlag == 1) {
    $(".noDataBox").hide()
    $(".listBox").show()
  } else {
    $(".noDataBox").show()
    $(".listBox").hide()
  }

  // 查询框
  $(".tabBox ul li").click(function () {
    console.log($(this).index() == 0 ? '个人' : '企业')
    $(this).addClass('tabActive');
    $(this).siblings().removeClass('tabActive');
    if ($(this).index() == 0) {
      $("#creTipLeftText").show()
      $("#creInput").show()
      $("#creInput2").hide()
    } else {
      $("#creTipLeftText").hide()
      $("#creInput").hide()
      $("#creInput2").show()
    }
  })

  // 1可修复，0不可修复
  function toDetail(val) {
    window.location.href = "./caseDetail.html?flag=" + val
  }

  // 全选
  var arr = [];
  //获取选中选项的值
  $("#allCheck").click(function () {
    var valArr = new Array;
    if (this.checked) {
      // $("#listBox :checkbox").attr("checked", true);
      $("input[name='checkbox']").prop("checked", true);
      $("input[name='checkbox']").each(function (i) {
        valArr[i] = $(this).val();
      });
      var vals = valArr.join(',');
      var valArrVal = valArr.filter(function (el) {
        return el !== '';
      });
      $(".checkedNum").empty().html("已选择" + valArrVal.length + "项")
    } else {
      // $("#listBox :checkbox").attr("checked", false);
      $("input[name='checkbox']").prop("checked", false);
      $(".checkedNum").empty().html("已选择0项")
    }

    arr = valArr
    console.log(arr)
  });

  //判断当全选时	若有一个不选 全选按钮为不选状态
  $('input[name="checkbox"]').change(function () {
    var checkedLength = $("input[name='checkbox']:checked").length;
    var checkLength = $("input[name='checkbox']").length;
    if (checkLength == checkedLength) {
      $("input[name='allCheckBox']").prop("checked", true);
      return true;
    } else {
      $("input[name='allCheckBox']").prop("checked", false);
      return true;
    }
  })
  $(".bottomListBox .itemBox").change(function () {
    if ($(this).find("input[type='checkbox']").is(':checked')) {
      if (arr.length == 0) {
        arr.push($(this).find("input[type='checkbox']").val())
      } else {
        for (let i = 0; i < arr.length; i++) {
          if ($(this).find("input[type='checkbox']").val() == arr[i]) {
            //
          } else {
            arr.push($(this).find("input[type='checkbox']").val())
          }
        }
      }
    } else {
      for (let i = 0; i < arr.length; i++) {
        if ($(this).find("input[type='checkbox']").val() == arr[i]) {
          arr.splice(i, 1)
        }
      }
    }
    $(".checkedNum").empty().html("已选择" + $("input[name='checkbox']:checked").length + "项")
  })

  // 在线修复   0一般失信，1严重失信
  function toApplicationFn(val) {
    if (val == 0) {
      $("#commonly").show()
      $("#serious").hide()
    } else {
      $("#commonly").hide()
      $("#serious").show()
    }
  }
  function toRestorationFn() {
    window.location.href = "./repairApplication.html"
  }

  // 批量修复
  $(".btnText").click(function () {
    var new_arr = [];
    for (var i = 0; i < arr.length; i++) {
      var items = arr[i];
      if ($.inArray(items, new_arr) == -1) {
        new_arr.push(items);
      }
    }
    console.log(new_arr);
  })

  // 去充值
  function toPackageFn() {
    window.location.href = "./topUpPackage.html"
  }

</script>

